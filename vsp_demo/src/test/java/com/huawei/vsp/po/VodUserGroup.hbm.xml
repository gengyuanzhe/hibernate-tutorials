<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.huawei.vsp.po">

    <class name="VodUserGroup" table="VOD_USERGROUPS">
        <composite-id>
            <key-property name="vodId" type="long" column="OBJECTID" />
            <key-property name="mediaId" type="string" column="MEDIAID" />
            <key-property name="groupId" type="string" column="GROUPID" />
        </composite-id>
        <property name="updateTime" type="string" column="UPDATETIME" />
    </class>

</hibernate-mapping>